{
  "functions": {
    "source": "dist/server"
  },
  "hosting": {
    "public": "dist/client",
    "ignore": [
      "**/.*",
      "firebase.json",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ],
    "headers": [
      {
        "source" : "**/*.@(jpg|jpeg|gif|png|ico|webp)",
        "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=604800"
        } ]
      }, {
        "source" : "**/*.@(js|css)",
        "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=604800"
        } ]
      }
    ]
  }
}
